{
  "name": "beatschain-extension",
  "version": "3.0.0",
  "description": "BeatsChain Chrome Extension with comprehensive upload and campaign management",
  "main": "popup/popup.js",
  "scripts": {
    "test": "node run-comprehensive-tests.js",
    "test:upload": "node run-comprehensive-tests.js --upload-only",
    "test:campaign": "node run-comprehensive-tests.js --campaign-only",
    "test:integration": "node test-upload-integration.html",
    "dev": "npm run test",
    "build": "node tools/zip-packager.sh",
    "lint": "echo 'Linting not configured'",
    "start": "echo 'Use Chrome extension developer mode to load extension'"
  },
  "keywords": [
    "chrome-extension",
    "music",
    "nft",
    "blockchain",
    "upload",
    "campaign-management"
  ],
  "author": "BeatsChain Team",
  "license": "MIT",
  "devDependencies": {
    "jsdom": "^22.1.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beatschain/chrome-extension"
  },
  "bugs": {
    "url": "https://github.com/beatschain/chrome-extension/issues"
  },
  "homepage": "https://beatschain.app"
}