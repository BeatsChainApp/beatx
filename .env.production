# Production environment variables for Railway
NODE_ENV=production
PORT=4000

# Supabase (configure in Railway dashboard)
SUPABASE_URL=${{SUPABASE_URL}}
SUPABASE_SERVICE_ROLE_KEY=${{SUPABASE_SERVICE_ROLE_KEY}}
SUPABASE_ANON_KEY=${{SUPABASE_ANON_KEY}}

# Livepeer
LIVEPEER_API_KEY=${{LIVEPEER_API_KEY}}
LIVEPEER_API_HOST=https://livepeer.studio/api

# IPFS/Pinning
WEB3STORAGE_TOKEN=${{WEB3STORAGE_TOKEN}}
PINATA_JWT=${{PINATA_JWT}}

# Thirdweb
THIRDWEB_SECRET_KEY=${{THIRDWEB_SECRET_KEY}}
THIRDWEB_CLIENT_ID=${{THIRDWEB_CLIENT_ID}}

# Google OAuth
GOOGLE_CLIENT_ID=${{GOOGLE_CLIENT_ID}}

# IPFS Proxy
IPFS_PROXY_GATEWAY=https://cloudflare-ipfs.com/ipfs/
IPFS_PROXY_ALLOWED_ORIGINS=*